<%- include('../partials/header') %>
<h1>Flight List</h1>
        <table>
            <thead>
            
                <tr>
                    <th>Airline</th>
                    <th>Flight #</th>
                    <th>Depature Time</th>
                </tr>
              
            </thead>
            <tbody>
              
                <% flights.forEach(function(flight) { %>
                  
              
                    <tr>
                    <td><%= flight.airline%></td>
                    <td><%= flight.flightNo%></td>
                    <td><%= flight.departs%></td>
                    </tr>
                <% });%>
             
            </tbody>
        </table>



<%- include('../partials/footer') %> 